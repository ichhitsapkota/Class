public class TestConstructor{
    String name;
    String address;
    String phoneNumber;
    TestConstructor()
    {
        name="Ichhit";
        System.out.println(name);
    }
    TestConstructor(String param1, String param2)
    {
        address=param1;
        phoneNumber=param2;
        System.out.println(address+"\n"+phoneNumber);
    }
    public static void main(String[] args){
        TestConstructor obj1 =new TestConstructor();
        TestConstructor obj2= new TestConstructor("Kathmandu","9844777350");
    }        
}
